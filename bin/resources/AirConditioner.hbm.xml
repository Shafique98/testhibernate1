<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.velociter.hibernate.airconditioner.AirConditioner" table="AIR_CONDITIIONER">
		<id name="id" column="ID">
		<generator class="increment"></generator>
		</id>
 		<property name="AcName" column="AC_NAME"></property>
		<property name="AcCapacity" column="AC_CAPACITY"></property>
		<property name="AcEnergyStars" column="AC_ENERGY_STAR"></property>
		<property name="AcColor" column="AC_COLOR"></property>
		<property name="warranty" column="WARRANTY"></property>
		<property name="specialFeature" column="SPL_FEATURE"></property>
		<property name="maxTemp" column="MAX_TEMP"></property>
		<property name="minTemp" column="MIN_TEMP"></property>
	</class>
</hibernate-mapping>