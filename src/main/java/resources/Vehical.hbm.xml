<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.velociter.hibernate.vehical.Vehical" table="VEHICAL">
		<id name="vehicalId" column="VEHICALID">
			<generator class="increment"></generator>
		</id>
		<property name="vehicalType" column="VEHICALTYPE" type="string"></property>
		<property name="chasisNumber" column="CHASISNUMBER" type="long"></property>
		<property name="companyName" column="COMPANYNAME" type="string"></property>
		<property name="vehicalNumber" column="VEHICALNUMBER" type="string"></property>
		<property name="modelType" column="MODELTYPE" type="string"></property>
		<property name="color" column="COLOR" type="string"></property>
		<property name="fuelType" column="FUELTYPE" type="string"></property>
		<property name="numberOfTyres" column="NUMBEROFTYRES" type="long"></property>
	</class>
</hibernate-mapping>